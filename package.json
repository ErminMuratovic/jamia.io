{
  "name": "jamia.io",
  "version": "1.0.0",
  "description": "Application for managing your Jamia.",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "compile": "npm run compile:ts",
    "compile:ts": "tsc",
    "clean": "rimraf dist/*",
    "precompile": "npm run clean",
    "lint": "jshint *.ts **/*.ts",
    "lint:checkstyle": "npm run lint --reporter checkstyle > checkstyle.xml",
    "test": "mocha --reporter spec --compilers ts:ts-node/register test/**/*.test.ts",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ermin-muratovic/jamia.io.git"
  },
  "keywords": [
    "jamia",
    "dzemat",
    "manager",
    "accounting"
  ],
  "author": "Ermin Muratovic",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ermin-muratovic/jamia.io/issues"
  },
  "homepage": "https://github.com/ermin-muratovic/jamia.io#readme",
  "dependencies": {
    "body-parser": "^1.16.0",
    "express": "^4.14.0",
    "mongoose": "^4.7.7",
    "morgan": "^1.7.0"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/chai": "^3.4.34",
    "@types/chai-http": "0.0.29",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.1",
    "chai": "^3.5.0",
    "debug": "^2.6.0",
    "jshint": "^2.9.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4",
    "ts-node": "^2.0.0",
    "typescript": "^2.1.5"
  }
}
